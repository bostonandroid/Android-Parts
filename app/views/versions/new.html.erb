<%= semantic_form_for [@package,@version] do |form| %>
  <%= form.inputs do %>
    <%= form.input :number %>
    <%= form.input :min_sdk %>
    <%= form.input :license %>
    <%= form.input :license_url %>
  <% end %>
  <%= form.buttons do %>
    <%= form.commit_button 'Publicize' %>
  <% end %>
<% end %>
